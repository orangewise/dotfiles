# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/ronald/opt/anaconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/ronald/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/ronald/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/ronald/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

# this is NOT the brew nvm
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh" # This loads nvm


source ~/.homebrew
source ~/.aliases
source ~/.python
source ~/.gitprompt
source ~/.rbenv
